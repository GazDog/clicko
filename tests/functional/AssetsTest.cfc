<cfcomponent extends="wheelsMapping.Test">

    <!---
    # SETUP & TEARDOWN
    --->

    <!--- include helper functions --->
    <cfinclude template="../helpers.cfm">
    
    <!--- setup runs before every test --->
    <cffunction name="setup">
        <!--- save the orginal environment --->
        <cfset loc.originalApplication = Duplicate(application)>
        <!--- set transaction mode to rollback, so no records are affected --->
        <cfset application.wheels.transactionMode = "rollback">
        <!--- a struct used to set valid model property values --->
        <cfset loc.validProperties = {
        	campaignid=1,
			publisherid=1,
			name='name_string',
			sourceurl='sourceurl_string',
			destinationurl='destinationurl_string',
			startat=createDateTime(2000,1,1,0,0,0),
			finishat=createDateTime(2000,1,1,0,0,0),
			notes='notes_string'
        }>
        <!--- a struct used to set invalid model property values --->
        <cfset loc.invalidProperties = {
        	campaignid='abcd',
			publisherid='abcd',
			name='xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx',
			sourceurl='xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx',
			destinationurl='xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx',
			startat='efgh',
			finishat='efgh',
			notes=''
        }>
    </cffunction>

    <!--- teardown runs after every test --->
    <cffunction name="teardown">
         <!--- re-instate the original application scope --->
        <cfset application = loc.originalApplication />
    </cffunction>

    <!---
    # INDEX
    --->

    <cffunction name="test_index_displays_asset_listing">
        <!--- setup some params for the tests --->
        <cfset loc.params = {controller="Assets", action="index"}>
        <!--- create an instance of the controller --->
        <cfset loc.controller = controller("Assets", loc.params)>
        <!--- process the action of the controller --->
        <cfset loc.controller.$processAction()>
        <!--- get copy of the code the view generated --->
        <cfset loc.response = loc.controller.response()>
        <!--- make sure this string is displayed  --->
        <cfset loc.string = '<h1>Listing Assets</h1>'>
        <cfset assert('loc.response contains loc.string')>
        <!--- check additional strings --->

    </cffunction>

    <!---
    # SHOW
    --->

    <cffunction name="test_show_displays_asset">
        <!--- find a asset object --->
        <cfset loc.asset = model("Asset").findOne()>
        <!--- define the key param --->
        <cfset loc.params = {controller="Assets", action="show", key=loc.asset.key()}>
        <cfset loc.controller = controller("Assets", loc.params)>
        <cfset loc.controller.$processAction()>
        <cfset loc.response = loc.controller.response()>
        <cfset loc.string = '<h1>Showing Asset</h1>'>
        <cfset assert('loc.response contains loc.string')>
        <!--- check additional strings --->

    </cffunction>

    <cffunction name="test_show_redirects_to_index_if_a_asset_is_not_found">
        <!--- provide a key that doesn't exist --->
        <cfset loc.params = {controller="Assets", action="show", key=-1}>
        <cfset loc.controller = controller(name="Assets", params=loc.params)>
        <cfset loc.controller.$processAction()>
        <cfset loc.redirect = loc.controller.$getRedirect()>
        <cfset assert('StructKeyExists(loc.redirect, "$args")')>
        <cfset assert('loc.redirect.$args.action eq "index"')>
    </cffunction>

    <!---
    # NEW
    --->

    <cffunction name="test_new_displays_asset_form">
        <cfset loc.params = {controller="Assets", action="new"}>
        <!--- create a new asset object for the form --->
        <cfset loc.params.asset = model("Asset").new()>
        <cfset loc.controller = controller("Assets", loc.params)>
        <cfset loc.controller.$processAction()>
        <cfset loc.response = loc.controller.response()>
        <cfset loc.string = '<h1>Create a New Asset</h1>'>
        <cfset assert('loc.response contains loc.string')>
        <!--- check additional strings --->

    </cffunction>

    <!---
    # EDIT
    --->

    <cffunction name="test_edit_displays_asset_form">
        <cfset loc.asset = model("Asset").findOne()>
        <cfset loc.params = {controller="Assets", action="edit", key=loc.asset.key()}>
        <cfset loc.controller = controller("Assets", loc.params)>
        <cfset loc.controller.$processAction()>
        <cfset loc.response = loc.controller.response()>
        <cfset loc.string = '<h1>Editing Asset</h1>'>
        <cfset assert('loc.response contains loc.string')>
        <!--- check additional strings --->

    </cffunction>

    <cffunction name="test_edit_redirects_to_index_if_a_asset_is_not_found">
        <cfset loc.params = {controller="Assets", action="edit", key=-1}>
        <cfset loc.controller = controller(name="Assets", params=loc.params)>
        <cfset loc.controller.$processAction()>
        <cfset loc.redirect = loc.controller.$getRedirect()>
        <cfset assert('StructKeyExists(loc.redirect, "$args")')>
        <cfset assert('loc.redirect.$args.action eq "index"')>
    </cffunction>

    <!---
    # CREATE
    --->

    <cffunction name="test_create_redirects_to_index_after_a_asset_is_created">
        <cfset loc.params = {controller="Assets", action="create"}>
        <!--- set valid properties for creating a asset --->
        <cfset loc.params.asset = loc.validProperties>
        <cfset loc.controller = controller(name="Assets", params=loc.params)>
        <cfset loc.controller.$processAction()>
        <cfset loc.redirect = loc.controller.$getRedirect()>
        <cfset assert('StructKeyExists(loc.redirect, "$args")')>
        <cfset assert('loc.redirect.$args.action eq "index"')>
    </cffunction>

    <cffunction name="test_create_errors_display_when_asset_is_invalid">
        <cfset loc.params = {controller="Assets", action="create"}>
        <!--- set invalid properties for creating a asset --->
        <cfset loc.params.asset = loc.invalidProperties>
        <cfset loc.controller = controller("Assets", loc.params)>
        <cfset loc.controller.$processAction()>
        <cfset loc.response = loc.controller.response()>
        <cfset loc.message = 'There was an error creating the asset'>
        <cfset assert('loc.response contains loc.message')>
    </cffunction>

    <!---
    # UPDATE
    --->

    <cffunction name="test_update_redirects_to_index_after_a_asset_is_updated">
        <cfset loc.asset = model("Asset").findOne()>
        <cfset loc.params = {controller="Assets", action="update", key=loc.asset.key()}>
        <cfset loc.params.asset = loc.asset.properties()>
        <cfset loc.controller = controller(name="Assets", params=loc.params)>
        <cfset loc.controller.$processAction()>
        <cfset loc.redirect = loc.controller.$getRedirect()>
        <cfset assert('StructKeyExists(loc.redirect, "$args")')>
        <cfset assert('loc.redirect.$args.action eq "index"')>
    </cffunction>

    <cffunction name="test_update_errors_display_when_asset_is_invalid">
        <cfset loc.asset = model("Asset").findOne()>
        <cfset loc.params = {controller="Assets", action="update", key=loc.asset.key()}>
        <cfset loc.params.asset = loc.invalidProperties>
        <cfset loc.controller = controller("Assets", loc.params)>
        <cfset loc.controller.$processAction()>
        <cfset loc.response = loc.controller.response()>
        <cfset loc.message = 'There was an error updating the asset'>
        <cfset assert('loc.response contains loc.message')>
    </cffunction>

    <!---
    # DELETE
    --->

    <cffunction name="test_delete_redirects_to_index_after_a_asset_is_deleted">
        <cfset loc.asset = model("Asset").findOne()>
        <cfset loc.params = {controller="Assets", action="delete", key=loc.asset.key()}>
        <cfset loc.controller = controller(name="Assets", params=loc.params)>
        <cfset loc.controller.$processAction()>
        <cfset loc.redirect = loc.controller.$getRedirect()>
        <cfset assert('StructKeyExists(loc.redirect, "$args")')>
        <cfset assert('loc.redirect.$args.action eq "index"')>
    </cffunction>

</cfcomponent>
