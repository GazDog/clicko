<cfcomponent extends="wheelsMapping.Test">
    
	<!--- include helper functions --->
	<cfinclude template="../helpers.cfm">

	<!--- setup runs before every test --->
    <cffunction name="setup">
    	<!--- save the orginal environment --->
		<cfset loc.originalApplication = Duplicate(application)>
		<!--- set transaction mode to rollback, so no records are affected --->
		<cfset application.wheels.transactionMode = "rollback">
        <!--- create an instance of our Asset --->
        <cfset loc.asset = model("Asset").new()>
        <!--- a struct used to set valid model property values --->
        <cfset loc.validProperties = {
        	campaignid=1,
			publisherid=1,
			name='name_string',
			sourceurl='sourceurl_string',
			destinationurl='destinationurl_string',
			startat=createDateTime(2000,1,1,0,0,0),
			finishat=createDateTime(2000,1,1,0,0,0),
			notes='notes_string'
        }>
        <!--- a struct used to set invalid model property values --->
        <cfset loc.invalidProperties = {
        	campaignid='abcd',
			publisherid='abcd',
			name='xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx',
			sourceurl='xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx',
			destinationurl='xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx',
			startat='efgh',
			finishat='efgh',
			notes=''
        }>
    </cffunction>
    
    <!--- teardown runs after every test --->
    <cffunction name="teardown">
    	 <!--- re-instate the original application scope --->
        <cfset application = loc.originalApplication />
    </cffunction>

    <!--- assert that setup and teardown pass --->
    <cffunction name="test_setup_and_teardown">  
		<cfset assert("true")>
	</cffunction>

	<!--- assert that because the properties are set correct and meet validation, the model is valid --->
    <cffunction name="test_asset_is_valid">
		<!--- set the properties of the model --->
		<cfset loc.asset.setProperties(loc.validProperties)>
		<cfset loc.result = loc.asset.valid()>
		<cfset assert("loc.result","loc.asset.allErrors()")>
	</cffunction>

	<!--- assert the model is invalid when no properties are set --->
	<cffunction name="test_asset_is_not_valid">
		<cfset loc.asset.setProperties(loc.invalidProperties)>
        <cfset assert("! loc.asset.valid()")>
	</cffunction>

	<!--- assert the model creates successfully --->
	<cffunction name="test_asset_create">
		<!--- set the properties of the model --->
		<cfset loc.asset.setProperties(loc.validProperties)>
		<cfset loc.result = loc.asset.save()>
        <cfset assert("loc.result", "loc.asset.allErrors()")>
	</cffunction>

	<!--- assert the model updates successfully --->
	<cffunction name="test_asset_update">
		<cfset loc.asset = model("asset").findOne()>
		<cfset loc.asset.setProperties(loc.validProperties)>
		<cfset loc.result = loc.asset.update()>
        <cfset assert("loc.result", "loc.asset.allErrors()")>
	</cffunction>

	<!--- assert the model deletes successfully --->
	<cffunction name="test_asset_delete">
		<cfset loc.asset = model("asset").findOne()>
		<cfset loc.result = loc.asset.delete()>
        <cfset assert("loc.result")>
	</cffunction>

</cfcomponent>
